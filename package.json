{
  "name": "hotel-panel",
  "scripts": {
    "lint": "eslint app",
    "lint-fix": "eslint ./app --fix",
    "postinstall": "npm run build:dll",
    "postsetup": "npm run build:dll",
    "build": "better-npm-run build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development node server",
    "graph": "babel-node --presets es2015 ./graph/index.js",
    "update-schema": "babel-node  --presets es2015 ./graph/scripts/updateSchema.js",
    "test": "jest"
  },
  "betterScripts": {
    "build": {
      "command": "BASE_URL=http://18.196.107.223:3000 GRAPH_URL=http://18.196.107.223:4000 WS_URL=ws://18.196.107.223:4010 webpack --verbose --colors --progress --display-error-details --config internals/webpack/webpack.prod.babel.js"
    }
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testEnvironment": "node",
    "transform": {
      ".+\\.js?$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    }
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "apisauce": "^0.14.1",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.8",
    "babel-polyfill": "^6.26.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "express": "4.14.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "immutable": "3.8.1",
    "intl": "1.2.5",
    "invariant": "2.2.2",
    "ip": "1.1.4",
    "lodash": "4.17.2",
    "material-ui": "^0.18.5",
    "minimist": "1.2.0",
    "moment": "2.18.1",
    "prop-types": "^15.6.1",
    "raven-js": "^3.25.1",
    "react": "15.4.1",
    "react-apollo": "^2.1.4",
    "react-dom": "15.4.1",
    "react-helmet": "3.2.2",
    "react-intl": "2.1.5",
    "react-notification-system": "^0.2.15",
    "react-notification-system-redux": "^1.1.4",
    "react-redux": "^5.0.6",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.6",
    "react-router-scroll": "0.4.1",
    "react-toastr-basic": "^1.1.14",
    "recompose": "0.25.0",
    "redux": "4.0.0",
    "redux-raven-middleware": "^1.2.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sanitize.css": "^5.0.0",
    "styled-components": "1.1.2",
    "subscriptions-transport-ws": "^0.9.9",
    "warning": "3.0.0",
    "whatwg-fetch": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
